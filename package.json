{
   "name": "server",
   "version": "1.0.0",
   "description": "A MERN stack movie website/application",
   "main": "dist/server.js",
   "scripts": {
      "dev": "concurrently 'npm run dev:server' 'npm run dev:client'",
      "dev:client": "npm --prefix ./client run dev",
      "dev:server": "ts-node-dev --respawn --pretty --transpile-only src/server.ts",
      "build": "npm run build:server && npm run build:client",
      "build:client": "npm --prefix ./client run build",
      "build:server": "swc src -d dist",
      "lint": "npm run lint:server && npm run lint:client",
      "lint:client": "npm --prefix ./client run lint",
      "lint:server": "eslint .",
      "lint:fix": "eslint . --fix",
      "format:check": "prettier --check './**/*.{md,js,json,ts,tsx}'",
      "format:write": "prettier --write './**/*.{md,js,json,ts,tsx}'",
      "start": "cross-env NODE_ENV=production node ."
   },
   "engines": {
      "node": ">=14.19.0"
   },
   "keywords": [
      "crud",
      "express",
      "react",
      "tailwindcss",
      "typescript",
      "vite"
   ],
   "author": "Kevin Silvester",
   "repository": {
      "type": "git",
      "url": "https://github.com/KevinSilvester/mern-movie.git"
   },
   "homepage": "https://github.com/KevinSilvester/mern-movie.git",
   "license": "MIT",
   "dependencies": {
      "axios": "^0.24.0",
      "config": "^3.3.6",
      "cors": "^2.8.5",
      "dotenv": "^10.0.0",
      "express": "^4.17.2",
      "helmet": "^5.0.1",
      "mongoose": "^6.1.5",
      "morgan": "^1.10.0",
      "nanoid": "^3.1.30",
      "nanoid-dictionary": "^4.3.0",
      "pino": "^7.6.2",
      "pino-pretty": "^7.3.0",
      "replace-special-characters": "^1.2.7",
      "zod": "^3.11.6"
   },
   "devDependencies": {
      "@eslint/create-config": "^0.4.1",
      "@swc/cli": "^0.1.57",
      "@swc/core": "^1.3.11",
      "@types/body-parser": "^1.19.2",
      "@types/config": "^0.0.41",
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.13",
      "@types/morgan": "^1.9.3",
      "@types/nanoid-dictionary": "^4.2.0",
      "@types/node": "^17.0.8",
      "@typescript-eslint/eslint-plugin": "^5.42.0",
      "@typescript-eslint/parser": "^5.42.0",
      "concurrently": "^7.5.0",
      "cross-env": "^7.0.3",
      "eslint": "^8.26.0",
      "nodemon": "^2.0.15",
      "prettier": "^2.7.1",
      "ts-node-dev": "^1.1.8",
      "typescript": "^4.5.4"
   }
}
