{
   "$schema": "https://json.schemastore.org/tsconfig",
   "display": "Default",
   "compilerOptions": {
      "allowJs": false,
      "allowSyntheticDefaultImports": true,
      "declaration": true,
      "declarationMap": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "isolatedModules": true,
      "moduleResolution": "node",
      "skipLibCheck": true,
      "strict": true
   },
   "exclude": ["node_modules"]
}
